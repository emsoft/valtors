{
  "parser": "babel-eslint", // https://github.com/babel/babel-eslint
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "no-console": "off",
    "max-len": "off",
    "linebreak-style": "off",
    "dot-notation": "off",
    "indent": "off"
  }
}
